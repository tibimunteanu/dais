{
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "files.associations": {
    "base_helper_macros.h": "c"
  },
  "todohighlight.defaultStyle": {
    "color": "#7f9f9a",
    "backgroundColor": "rgba(0,0,0,0)",
    "overviewRulerColor": "rgba(0,0,0,0)",
    "isWholeLine": false
  },
  "todohighlight.keywords": [
    "global ",
    "prv ",
    "pub ",
    "fn ",
    "inl ",
    "noinl ",
    "catch",
    "assert",
    "staticAssert",
    { "text": "unreachable", "color": "#df6a6a" },
    { "text": "logFatal", "color": "#df6a6a" },
    { "text": "logError", "color": "#df6a6a" },
    { "text": "ERROR", "color": "#df6a6a" },
    { "text": "try", "color": "#df6a6a" },
    { "text": "error", "color": "#df6a6a" },
    { "text": "alert", "color": "#ff8800" },
    { "text": "VK_SUCCESS", "color": "#91ab62" },
    { "text": "VK_INCOMPLETE", "color": "#ff8800" },
    { "text": "INCOMPLETE", "color": "#ff8800" },
    { "text": "OK", "color": "#91ab62" },
    { "text": "ok", "color": "#91ab62" },
    { "text": "noerr", "color": "#91ab62" },
    { "text": "API ", "color": "#1f9cf0" },
    { "text": "BRIEF", "color": "#99ffa0", "isWholeLine": true }
  ]
}
